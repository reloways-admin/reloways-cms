{
  "kind": "collectionType",
  "collectionName": "knowledge_articles",
  "info": {
    "singularName": "knowledge-article",
    "pluralName": "knowledge-articles",
    "displayName": "Knowledge Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "Slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "Title",
      "required": true
    },
    "Content": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "Category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::knowledge-category.knowledge-category"
    },
    "Order": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::person.person"
    },
    "verifiedBy": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::person.person"
    },
    "verifiedAt": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
